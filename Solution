#include<iostream>
#include<cmath>
using namespace std;
int main(){
int x,y,z,T,c,i;
cin >>T;
c=1;
for(i=0;i<T;i++){
cin >> x >> y >> z;
if(x<y && y<z ||x>y && y>z)
cout <<"Case"<<c<<":"<<y<<endl;

else if (y<x && x<z || y>x && x>z)
cout <<"Case"<<c<<":"<<x<<endl;

else if (x<z && z<y || x>z && z>y)
cout <<"Case "<<c<<":"<<z<<endl;
c++;
}
return 0;}
